@prefix rr: <http://www.w3.org/ns/r2rml#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix rml: <http://semweb.mmlab.be/ns/rml#>.
@prefix ql: <http://semweb.mmlab.be/ns/ql#>.
@prefix schema: <https://schema.org/>.
@prefix ex: <http://ex.com/>.
@prefix sql: <http://w3.org/ns/sql#>.
<#EMPLOYED>
  rr:subjectMap [
  	rr:template "http://ex.com/employed/{id}";
  	rr:class schema:Person
  ];
  rml:logicalSource [
  	rml:source "data/employeds.csv";
  	rml:referenceFormulation ql:csv
  ];
  rr:predicateObjectMap [
  	rr:predicateMap [ rr:constant schema:name];
  	rr:objectMap [ rml:termType rr:literal; rml:reference "name"; rr:datatype xsd:string]
  ];
  rr:predicateObjectMap [
  	rr:predicateMap [ rr:constant schema:jobTitle];
  	rr:objectMap [ rml:termType rr:literal; rml:reference "role"; rr:datatype xsd:string]
  ];
  rr:predicateObjectMap [
  	rr:predicateMap [ rr:constant schema:description];
  	rr:objectMap [ rml:termType rr:literal; rml:reference "bio"; rr:datatype xsd:string]
  ];
  rr:predicateObjectMap [
  	rr:predicateMap [ rr:constant schema:image];
  	rr:objectMap [ rml:termType rr:literal; rml:reference "img"; rr:datatype xsd:string]
  ];
.
<#NEW>
  rr:subjectMap [
  	rr:template "http://ex.com/news/{news_id}";
  	rr:class schema:Article
  ];
  rml:logicalSource [
  	rml:source "data/news.csv";
  	rml:referenceFormulation ql:csv
  ];
  rr:predicateObjectMap [
  	rr:predicateMap [ rr:constant schema:name];
  	rr:objectMap [ rml:termType rr:literal; rml:reference "title"; rr:datatype xsd:string]
  ];
  rr:predicateObjectMap [
  	rr:predicateMap [ rr:constant schema:creator];
  	rr:objectMap [ rml:termType rr:literal; rml:reference "author"; rr:datatype xsd:string]
  ];
  rr:predicateObjectMap [
  	rr:predicateMap [ rr:constant schema:image];
  	rr:objectMap [ rml:termType rr:literal; rml:reference "img"; rr:datatype xsd:string]
  ];
  rr:predicateObjectMap [
  	rr:predicateMap [ rr:constant schema:about];
  	rr:objectMap [ rml:termType rr:literal; rml:reference "preview"; rr:datatype xsd:string]
  ];
  rr:predicateObjectMap [
  	rr:predicateMap [ rr:constant schema:contentReferenceTime];
  	rr:objectMap [ rml:termType rr:literal; rml:reference "date"; rr:datatype xsd:string]
  ];
  rr:predicateObjectMap [
  	rr:predicateMap [ rr:constant schema:articleBody];
  	rr:objectMap [ rml:termType rr:literal; rml:reference "article"; rr:datatype xsd:string]
  ];
.
